function plot(points)
    scatter3( points(:,1),points(:,2),points(:,3),'k' )
    quiver3( points(:,1),points(:,2),points(:,3),...
             points(:,4),points(:,5),points(:,6),   'r' )
    quiver3( points(:,1),points(:,2),points(:,3),...
             points(:,7),points(:,8),points(:,9),   'g' )
    quiver3( points(:,1),points(:,2),points(:,3),...
             points(:,10),points(:,11),points(:,12),   'b' )
end




